#include "main.h"
#include <stdlib.h>
#include <stdio.h>

/**
 * main - prints the multiplication of two integers
 * @argc: argument count
 * @argv: argument vector
 * Return: 0 if true, 1 if false
 */

int main(int argc, char *argv[])

{
int cee, jay;

if (argc == 3)

{

cee = atoi(argv[1]);

jay = atoi(argv[2]);

printf("%d\n", cee * jay);

return (0);

}

printf("Error\n");

return (1);

}
